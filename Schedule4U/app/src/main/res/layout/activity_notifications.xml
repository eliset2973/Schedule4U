        android:layout_marginLeft="5dp"
        android:layout_marginTop="118dp"
        android:background="#007A0019"
        app:layout_constraintStart_toEndOf="@+id/notification_1"
        app:layout_constraintTop_toBottomOf="@+id/toolbar"
        app:srcCompat="@android:drawable/ic_menu_delete" />

    <ImageButton
        android:id="@+id/image_button_2"
        android:layout_width="61dp"
        android:layout_height="43dp"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="190dp"
        android:background="#007A0019"
        app:layout_constraintStart_toEndOf="@+id/notification_2"
        app:layout_constraintTop_toBottomOf="@+id/image_button_1"
        app:srcCompat="@android:drawable/ic_menu_delete" />


